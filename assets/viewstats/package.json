{
  "name": "viewstats",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn run dev && yarn run prod",
    "prod": "vue-cli-service build",
    "dev": "vue-cli-service build --mode developement",
    "lint": "vue-cli-service lint",
    "watch": "vue-cli-service build --mode=developement --watch"
  },
  "dependencies": {
    "@statnett/vue-plotly": "^0.3.2",
    "canvas-toBlob": "^1.0.0",
    "core-js": "^2.6.5",
    "d3": "^5.9.7",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.15",
    "plotly.js": "^1.49.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.21.1",
    "vuewordcloud": "^18.7.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "globals": {
      "_": true
    },
    "env": {
      "node": true,
      "jquery": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
